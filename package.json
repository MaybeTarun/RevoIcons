{
  "name": "revoicons",
  "version": "2.0.0",
  "description": "Icon Library",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./icons/*": {
      "types": "./dist/index.d.ts",
      "import": "./dist/icons/*.mjs",
      "require": "./dist/icons/*.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaybeTarun/RevoIcons.git"
  },
  "keywords": [
    "revo",
    "icons",
    "library",
    "svg",
    "react",
    "nextjs"
  ],
  "author": "Tarun Gupta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaybeTarun/RevoIcons/issues"
  },
  "homepage": "https://github.com/MaybeTarun/RevoIcons#readme",
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
